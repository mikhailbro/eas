          "iban": {
            "type": "string"
          },
          "reference": {
            "type": "string"
          }
        }
      },
      "Commencement": {
        "type": "object",
        "description": "Eintritt",
        "properties": {
          "uid": {
            "type": "string"
          },
          "sozv": {
            "type": "string"
          },
          "commencementAt": {
            "type": "string",
            "format": "date-time"
          },
          "birthday": {
            "type": "string",
            "format": "date-time"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "sex": {
            "type": "string",

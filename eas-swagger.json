          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "sex": {
            "type": "string",
            "enum": [
              "M",
              "W",
              "UNKNOWN"
            ]
          },
          "internalRef": {
            "type": "string"
          },
          "expiresAt": {
            "type": "string",
            "format": "date-time"
          },
          "termination-id": {
            "type": "string"
          }
        },
        "required": [
          "uid",
          "sozv",
          "terminationAt",
          "birthday",
          "firstName",
          "lastName",
          "sex",
          "internalRef",
          "termination-id"
        ]
      },
      "Match": {
        "type": "object",
        "description": "E/A tinder ;)",
        "properties": {
          "id": {
            "type": "string"
          },
          "commencement": {
            "$ref": "#/components/schemas/Commencement"
          },
          "termination": {
            "$ref": "#/components/schemas/Termination"
          },
          "sendingPf": {
            "$ref": "#/components/schemas/PensionFund"
          },
          "receivingPf": {
            "$ref": "#/components/schemas/PensionFund"
          },
          "paymentInfo": {
            "$ref": "#/components/schemas/PaymentInfo"
          },
          "status": {
            "type": "string",
            "enum": [
              "OPEN",
              "ACCEPTED_BY_RECEIVER",
              "REJECTED_BY_RECEIVER",
              "REJECTED_BY_SENDER",
              "CLOSED_BY_SENDER"
            ]
          },
          "expiresAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "PensionFund": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "street": {
            "type": "string"
          },
          "zip": {
            "type": "string"
          },
          "city": {
            "type": "string"
          }
        },
        "description": "Vorsorgeeinrichtung (VE)",
        "title": "PensionFund"
      }
    },

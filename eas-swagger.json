      "Termination": {
        "type": "object",
        "description": "Austritt",
        "properties": {
          "uid": {
            "type": "string"
          },
          "sozv": {
            "type": "string"
          },
          "terminationAt": {
            "type": "string",
            "format": "date-time"
          },
          "birthday": {
            "type": "string",

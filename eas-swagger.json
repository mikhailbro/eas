              "W",
              "UNKNOWN"
            ]
          },
          "internalRef": {
            "type": "string"
          },
          "expiresAt": {
            "type": "string",
            "format": "date-time"
          },
          "commencement-id": {
            "type": "string"
          }
        },
        "required": [
          "uid",
          "sozv",
          "commencementAt",
